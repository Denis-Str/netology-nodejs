<%- include('../port/layout-start', {title: title}) %>
<%- include('../_components/menu') %>
<% if (books.length > 0) { %>
    <h4>Books list</h4>
    <ul class="list-group">
        <% books.forEach(book => { %>
            <li class="list-group-item"><%= book.title %></li>
        <% }); %>
    </ul>
<% } else { %>
    <h5>The list of books is empty</h5>
<% } %>
<%- include('../port/layout-end') %>